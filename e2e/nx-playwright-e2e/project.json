{
  "name": "nx-playwright-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/nx-playwright-e2e/src",
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn nx run nx-playwright:build",
          "SWC_NODE_PROJECT=e2e/nx-playwright-e2e/tsconfig.json node -r @swc-node/register --test e2e/nx-playwright-e2e/tests/nx-playwright.spec.ts"
        ],
        "parallel": false
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run nx-playwright:build"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nx-playwright"]
}
