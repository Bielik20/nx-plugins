{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "NxPlaywrightInit",
  "title": "Initialize the @ns3/nx-playwright plugin",
  "type": "object",
  "properties": {
    "skipFormat": {
      "description": "Skip formatting files",
      "type": "boolean",
      "default": false
    },
    "skipPackageJson": {
      "type": "boolean",
      "default": false,
      "description": "Do not add dependencies to `package.json`.",
      "x-priority": "internal"
    }
  },
  "required": []
}
